{
    "name": "votong/laravel-activitylog-mongodb",
    "description": "A very simple activity logger to monitor the users of your website or application",
    "keywords": [
        "votong",
        "log",
        "user",
        "activity",
        "laravel"
    ],
    "homepage": "https://github.com/votong/laravel-activitylog-mongodb",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Tom Witkowski",
            "email": "dev.gummibeer@gmail.com",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7|^8",
        "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/database": "^8.69 || ^9.27 || ^10.0 || ^11.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "spatie/laravel-package-tools": "^1.6.3",
        "jenssegers/mongodb": "^3|^4"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^6.23 || ^7.0 || ^8.0 || ^9.0",
        "pestphp/pest": "^1.20 || ^2.0",
        "phpunit/phpunit": "^9|^10|^11"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Votong\\Activitylog\\ActivitylogServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Votong\\Activitylog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Votong\\Activitylog\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
